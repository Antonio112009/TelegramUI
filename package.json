{
  "name": "@telegram-apps/telegram-ui",
  "version": "2.1.13",
  "description": "World-class, ultimate UI developer toolkit.",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "sideEffects": [
    "./dist/index.js",
    "./dist/cjs/index.js"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "concurrently \"npm test\" \"npm run build:types\" \"npm run swc:*\"",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "build:types": "concurrently 'npm run tsc:*'",
    "swc-base": "cross-env NODE_ENV=production swc src/ --config-file package.swcrc --extensions .tsx,.jsx,.ts,.js",
    "swc:es6": "npm run swc-base -- -d dist -s",
    "swc:cjs": "npm run swc-base -- -d dist/cjs -s -C module.type=commonjs",
    "tsc:es6": "cross-env NODE_ENV=production tsc --emitDeclarationOnly --declaration -p tsconfig.dist.json && tsc-alias",
    "tsc:cjs": "cross-env NODE_ENV=production tsc --emitDeclarationOnly --declaration --outDir dist/cjs/ -p tsconfig.dist.json && tsc-alias",
    "lint": "eslint src/ --max-warnings=0",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md,mdx}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md,mdx}\"",
    "dev": "storybook dev -p 6006 --docs",
    "storybook:build": "storybook build --docs",
    "storybook:publish": "rsync ./storybook-static/ xelene:/var/www/tgui.xelene.me -avz --exclude .git/ --exclude node_modules/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/telegram-mini-apps-dev/TelegramUI.git"
  },
  "author": "Ilya Smirnov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/telegram-mini-apps-dev/TelegramUI/issues"
  },
  "homepage": "https://github.com/telegram-mini-apps-dev/TelegramUI#readme",
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.1",
    "@eslint/js": "^9.39.2",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.8",
    "@twa-dev/types": "^8.0.2",
    "@types/eslint": "^9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^4.0.16",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "10.1.11",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "storybook": "^10.1.11",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.1.6",
    "@swc/helpers": "^0.5.18",
    "vaul": "^1.1.2"
  },
  "resolutions": {
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
